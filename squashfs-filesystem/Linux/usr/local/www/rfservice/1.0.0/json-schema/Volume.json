                },
                {
                    "$ref": "/redfish/v1/Schemas/Volume.v1_0_1.json#/definitions/Volume"
                },
                {
                    "$ref": "/redfish/v1/Schemas/Volume.v1_0_2.json#/definitions/Volume"
                },
                {
                    "$ref": "/redfish/v1/Schemas/Volume.v1_0_3.json#/definitions/Volume"
                }
            ]
        }
